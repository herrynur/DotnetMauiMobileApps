<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppNew.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppNew"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:NewsHomePage}"
        Route="NewsPage" />
    <ShellContent
        Title="Detail"
        ContentTemplate="{DataTemplate local:NewsDetailPage}"
        Route="NewsDetail" />

</Shell>
